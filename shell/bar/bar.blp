using Gtk 4.0;
using Astal 4.0;

template $Bar: Astal.Window {
    styles [
        "bar",
    ]

    CenterBox {
        [start]
        Label {
            label: "left";
        }

        [center]
        Label {
            label: "middle";
        }

        [end]
        Label {
            label: "right";
        }
    }
}
